{
  "headers": [
    {
      "source": "/(.*)",
      "headers" : [
        {
          "key" : "X-Content-Type-Options",
          "value" : "nosniff"
        },
        {
          "key" : "Referrer-Policy",
          "value" : "origin-when-cross-origin"
        },
        {
          "key" : "X-Frame-Options",
          "value" : "SAMEORIGIN"
        },
        {
          "key": "Strict-Transport-Security",
          "value": "max-age=2592000"
        }
      ]
    }
  ],
  "rewrites": [
    { "source": "/crab-home-v1/economic", "destination": "/index.html" },
    { "source": "/crab-home-v1/plo", "destination": "/index.html" },
    { "source": "/crab-home-v1/grants", "destination": "/index.html" },
    { "source": "/crab-home-v1/plo_contribute", "destination": "/index.html" }
  ]
}

